<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="110dp"
    android:orientation="horizontal">
    <RelativeLayout
        android:id="@+id/relative_manager_buquan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="20dp"

        android:layout_gravity="center_vertical"
        >
        <TextView
            android:id="@+id/tv_item_sh"
            android:layout_centerVertical="true"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:text="shanghu "
            android:textSize="22sp"
            android:textColor="#000"
            android:layout_marginBottom="33dp"
            android:layout_marginTop="33dp"
            />
        <View
            android:layout_toRightOf="@id/tv_item_sh"
            android:id="@+id/tv_line1"
            android:layout_width="1dp"
            android:layout_marginRight="20dp"
            android:layout_centerVertical="true"
            android:layout_height="20dp"
            android:background="#000"/>
        <TextView
            android:layout_toRightOf="@id/tv_line1"
            android:id="@+id/tv_item_pinpai"
            android:layout_centerVertical="true"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="待补全"
            android:textSize="22sp"
            android:layout_marginRight="20dp"
            android:textColor="#000"
            android:layout_marginBottom="33dp"
            android:layout_marginTop="33dp"
            />
        <View
            android:layout_toRightOf="@id/tv_item_pinpai"
            android:id="@+id/tv_line"
            android:layout_width="1dp"
            android:layout_marginRight="20dp"
            android:layout_centerVertical="true"
            android:layout_height="20dp"
            android:background="#000"/>
        <TextView
            android:layout_marginBottom="33dp"
            android:layout_marginTop="33dp"
            android:id="@+id/tv_item_vin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="0辆"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@id/tv_line"
            android:textSize="22sp"
            android:textColor="#000"
            />
        <ImageView
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:src="@drawable/icon_next"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            />
    </RelativeLayout>

</LinearLayout>
